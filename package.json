    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "pinia": "^2.3.1",
    "prisma": "^7.2.0",
    "vue": "^3.5.24",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
